<!doctype html>
<html>
<head>
	<title>dihh</title>
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
	<link rel="stylesheet" href="/style.css">
	<script src="/head.libs.js"></script>
</head>
<body>

<div class="main">
    <div class="scene">
        <div id="cratedigger" data-cratedigger-page="<%= pageId %>">
            <div id="cratedigger-canvas"></div>
            <div id="cratedigger-loading">
                <div class="info-container">
                    <div class="vertical-center">
                        <span class="loading-label">Loading records...</span>
                    </div>
                </div>
            </div>
            <div id="cratedigger-info">
                <div class="info-container">
                    <div class="vertical-center">
                        <div id="cratedigger-record-artist" class="cratedigger-label"></div>
                        <div id="cratedigger-record-title" class="cratedigger-label"></div>
                        <div id="cratedigger-record-cover" class="cratedigger-label"></div>
                        <div class="cratedigger-label">
                            <a id="cratedigger-record-listen" href="#">Listen</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="bottom-bar">
            <div class="bottom-bar-item bottom-bar-prev-button">
                PREVIOUS
            </div>
            <div class="bottom-bar-item full-centered bottom-bar-mid-button">
                SHOW
            </div>
            <div class="bottom-bar-item full-centered bottom-bar-next-button">
                NEXT
            </div>
        </div>
    </div>
    <main class="right-column">
        <div class="track">
            <div class="track-inner">
                <div class="track-header">
                    <div class="column-container">
                        <div class="track-artist"></div>
                        <div class="track-title"></div>
                    </div>
                </div>
                <div class="track-player">
                    <div id="player"></div>
                    <div class="label-loading track-player-loading">
                        <div class="track-player-loading-icon">
                            <i class="fa fa-refresh fa-spin"></i>
                        </div>
                        <div class="track-player-loading-label">Loading...</div>
                    </div>
                </div>
                <div class="track-controls">
                    <ul class="track-controls-buttons">
                        <li class="track-control-button button-fullwidth button-prev-track">
                            Prev
                        </li>
                        <li class="track-control-button button-fullwidth button-playpause">
                            Play
                        </li>
                        <li class="track-control-button button-fullwidth button-next-track">
                            Next
                        </li>
                    </ul>
                </div>
                <div class="track-infos">
                    <div class="track-info">
                        <span class="track-info-label">Label</span>
                        <span class="track-label"></span>
                    </div>
                    <div class="track-info">
                        <span class="track-info-label">Released</span>
                        <span class="track-year"></span>
                    </div>
                    <div class="track-description"></div>
                    <div class="track-links"></div>
                    <div id="disqus_thread"></div>
                </div>
            </div>
        </div>
        <div class="footer">
            <a href="#" class="button button-black button-prev-page">Prev Page</a>
            <span href="#" class="button button-black">Page <span class="page-number"><%= pageId %></span>/<%= pagesCount %></span>
            <a href="#" class="button button-black button-next-page">Next Page</a>
        </div>
    </main>
</div>

<script type="text/javascript" src="/libs.min.js"></script>
<script type="text/javascript" src="/app.min.js"></script>
<script type="text/javascript">
$(function() {
	App.init();
});

    /* * * CONFIGURATION VARIABLES * * */

    
</script>
</body>
</html>