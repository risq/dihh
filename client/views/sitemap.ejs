<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<% for(var i=1; i <= pagesCount; i++) {%>
		<url>
			<% if (i === 1) { %>
				<loc>http://dihh.io</loc>
			<% } else { %>
				<loc>http://dihh.io/page/<%= i %></loc>
			<% } %>
			<lastmod><%= lastModificationDate.toJSON().slice(0, 10) %></lastmod>
			<changefreq>weekly</changefreq>
			<priority>1</priority>
		</url>
	<% } %>
	<% for(var i=0; i < digs.length; i++) {%>
		<url>
			<loc>http://dihh.io/digs/<%= digs[i].slug %></loc>
			<lastmod><%= digs[i].created_at.toJSON().slice(0, 10) %></lastmod>
			<changefreq>monthly</changefreq>
			<priority>0.2</priority>
		</url>
	<% } %>
</urlset> 